<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
		  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">

	<activeProfiles>
		<activeProfile>github</activeProfile>
	</activeProfiles>

	<profiles>
		<profile>
			<id>github</id>
			<repositories>
				<repository>
					<id>github-solace-integration-test-support</id>
					<url>https://maven.pkg.github.com/SolaceDev/solace-integration-test-support</url>
				</repository>
			</repositories>
		</profile>
	</profiles>

	<servers>
		<server>
			<id>github-solace-integration-test-support</id>
			<username>${env.SOLACEDEV_GITHUB_USERNAME}</username>
			<password>${env.SOLACEDEV_GITHUB_TOKEN}</password>
		</server>
		<server>
			<id>github</id>
			<username>${env.GITHUB_ACTOR}</username>
			<password>${env.GITHUB_TOKEN}</password>
		</server>
		<server>
			<id>ossrh</id>
			<username>${env.MAVEN_OSSRH_USER}</username>
			<password>${env.MAVEN_OSSRH_PASS}</password>
		</server>
		<server>
			<id>gpg.passphrase</id>
			<passphrase>${env.GPG_PASSPHRASE}</passphrase>
		</server>
	</servers>
</settings>
